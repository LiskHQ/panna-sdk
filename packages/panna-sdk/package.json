    "@types/react": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^9.28.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
